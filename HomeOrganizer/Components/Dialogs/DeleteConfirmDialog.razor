<MudDialog DefaultFocus="DefaultFocus.LastChild">
    <DialogContent>
        <MudText Typo="Typo.body1">
            Are you sure to delete @Feature.TileData.UserGivenName of type @Feature.FeatureData.Name?
        </MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel" StartIcon="@Icons.Material.Filled.ArrowBack" Variant="Variant.Outlined"> Cancel </MudButton>
        <MudSpacer />
        <MudButton Color="Color.Error" OnClick="Delete" EndIcon="@Icons.Material.Filled.DeleteForever" Variant="Variant.Outlined">Delete</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    [Parameter]
    public IFeature Feature { get; set; }

    void Delete() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}